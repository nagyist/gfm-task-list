  "license": "MIT",
  "bugs": {
    "url": "https://github.com/waffleio/gfm-task-lists/issues"
  },
  "homepage": "https://github.com/waffleio/gfm-task-lists#readme",
  "devDependencies": {
    "awesome-typescript-loader": "^1.0.0",
    "bootstrap": "^4.0.0-alpha.2",
    "bootstrap-loader": "^1.0.10",
    "css-loader": "^0.23.1",
    "del-cli": "^0.2.0",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.21.0",
    "jquery": "^3.0.0",
    "node-sass": "^3.7.0",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "superagent": "^2.0.0",
    "tether": "^1.3.2",
    "typescript": "^1.8.10",
    "typings": "^1.2.0",
