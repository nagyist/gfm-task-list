  "description": "JavaScript module for enabling GFM task lists for third-party integrations to GitHub",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --display-chunks --progress",
    "clean": "./node_modules/.bin/del-cli ./target",
    "dev": "NODE_ENV=development webpack-dev-server --config=webpack/webpack.dev.config.js --inline --progress --profile --colors --watch --display-error-details --display-cached --hot",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waffleio/gfm-task-lists.git"
  },
  "keywords": [
